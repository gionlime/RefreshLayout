<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="lib_refresh_RefreshLayout">
        <attr name="enable" format="boolean" tools:ignore="ResourceName" />
        <attr name="gravity" tools:ignore="ResourceName">
            <flag name="top" value="1" />
            <flag name="bottom" value="2" />
            <flag name="left" value="4" />
            <flag name="right" value="8" />
        </attr>
    </declare-styleable>
</resources>
